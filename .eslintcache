                onChange={handleInputChange}\n                        />\n                        <br />\n                        <button\n                            type=\"submit\"\n                            className=\"btn btn-block btn-outline-primary\"\n                        >\n                            Search\n                        </button>\n                    </form>\n                </div>\n                <div className=\"col-7\">\n                    <h4> Results </h4>\n                    <hr />\n                    \n                    {\n                        (q === '')\n                            &&\n                            <div className=\"alert alert-info\">\n                                Search Your Next Hero!\n                            </div>\n                    } \n                    {\n                        (q !== '' && heroesFiltered.length === 0 )\n                            &&\n                            <div className=\"alert alert-danger\">\n                                We can't find any Hero with <b>{ q }</b> name!\n                            </div>\n                    } \n                    {\n                        heroesFiltered.map( hero => (\n                            <HeroCard key={hero.id} {...hero} />\n                        ))\n                    }\n                </div>\n            </div>\n        </div>\n    )\n}\n","/Users/omar/Desktop/Projects/React/07-heroes-app/src/routers/DashboardRoutes.jsx",[],"/Users/omar/Desktop/Projects/React/07-heroes-app/src/selectors/getHeroeById.jsx",[],"/Users/omar/Desktop/Projects/React/07-heroes-app/src/components/marvel/MarvelScreen.jsx",[],"/Users/omar/Desktop/Projects/React/07-heroes-app/src/types/types.jsx",[],"/Users/omar/Desktop/Projects/React/07-heroes-app/src/hooks/useForm.jsx",[],"/Users/omar/Desktop/Projects/React/07-heroes-app/src/selectors/getHeroesByName.jsx",[],"/Users/omar/Desktop/Projects/React/07-heroes-app/src/selectors/getHeroesByPublish